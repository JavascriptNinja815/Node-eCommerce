    <div id="page-wrapper">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Products</h1>
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->
        <div class="row">
            <div class="col-lg-12">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        Product Inventory
                    </div>
                    <!-- /.panel-heading -->
                    <div class="panel-body">
                        <table width="100%" class="table table-striped table-bordered table-hover dataTables">
                            <thead>
                                <tr>
                                    <th>Product Item</th>
                                    <th>Category</th>
                                    <th>Price <strong>$</strong></th>
                                    <th>Weight</th>
                                    <th>Order Details</th>
                                </tr>
                            </thead>
                            
                              <tbody>
                                  <% products.forEach(function(product) { %>
                                  <tr class="odd gradeX">
                                      <td><%= product.item %></td>
                                      <td><%= product.category %></td>
                                      <td>$<%= product.price %></td> 
                                      <td><%= product.weight %> Pounds</td>
                                      <td class="center"><button class="btn btn-md btn-success"><a href="/productsAdmin/<%= product._id %>" class="admin--button">Product Details</a></button></td>
                                  </tr>
                                  <% }) %> 
                              </tbody>
                              
                        </table>
                    </div>
                    <!-- /.panel-body -->
                </div>
                <!-- /.panel -->
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->
        
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Orders</h1>
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->
        <div class="row">
            <div class="col-lg-12">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        Recent Orders
                    </div>
                    <!-- /.panel-heading -->
                    <div class="panel-body">
                        <table width="100%" class="table table-striped table-bordered table-hover dataTables">
                            <thead>
                                <tr>
                                    <th>Payment ID</th>
                                    <th>Product Price <strong>$</strong></th>
                                    <th>Shipping Rate <strong>$</strong></th>
                                    <th>Full Name</th>
                                    <th>Order Details</th>
                                </tr>
                            </thead>
                            
                              <tbody>
                                  <% orders.forEach(function(order) { %>
                                  <tr class="odd gradeX">
                                      <td><%= order.paymentId %></td>
                                      <td>$<%= order.totalPrice %></td>
                                      <td>$<%= order.rate %></td> 
                                      <td><%= order.nameFirst %> <%= order.nameLast %></td>
                                      <td class="center"><button class="btn btn-md btn-primary"><a href="#" class="admin--button">Order Details</a></button></td>
                                  </tr>
                                  <% }) %> 
                              </tbody>
                              
                        </table>
                    </div>
                    <!-- /.panel-body -->
                </div>
                <!-- /.panel -->
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->

    </div>
